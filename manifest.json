{
    "name": "Webclock",
    "description": "A clock on web page to be aware of timing.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "/icons/clock16.png",
        "32": "/icons/clock32.png",
        "48": "/icons/clock48.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {}
}